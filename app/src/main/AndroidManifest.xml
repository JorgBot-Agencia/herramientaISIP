        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".GestionDocumental.GestionDocumental"></activity>
        <activity android:name=".cargando" />
        <activity android:name=".buscar_empresa" />
        <activity android:name=".infoDetallada" />
        <activity android:name=".registroEmpresa" />
        <activity android:name=".opcionPrincipal" />
        <activity android:name=".preguntasEncuesta" />
        <activity android:name=".registroFundacion" />
        <activity
            android:name=".menuprincipal"
            android:label="@string/title_activity_me"
            android:theme="@style/NoActionBar" />
        <activity android:name=".informacionEmpresarial" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>